<% breadcrumb :project_issues %>
<div class="row">
<div class="large-10 columns">
<h1>Listing Project Issues</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Project</th>
      <th>Summary</th>
    </tr>
  </thead>

  <tbody>
    <% @project_issues.each do |project_issue| %>
      <tr>
        <td><%= project_issue.name %></td>
        <td> <%= Project.find(project_issue.project_id).name %></td>
        <td><%= project_issue.summary %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project issue', new_project_issue_path, :class => 'small button radius' %>
</div>
</div>
<br />
<br />