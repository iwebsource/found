<% breadcrumb :registration, @registration %>
<div class="row">
<div class="large-3 columns">
<h3><i class="fi-torso"></i> Sign up</h3>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div><%= f.email_field :email, :autofocus => true, :placeholder => "Type Email Here" %></div>

  <div><%= f.password_field :password, :placeholder => "Enter Password" %></div>

  <div><%= f.password_field :password_confirmation, :placeholder => "Retype Password" %></div>

  <div><%= f.submit "Sign up" %></div>
<% end %>

<br />

<%= render "devise/shared/links" %>
<br />
<br />
</div>
</div>